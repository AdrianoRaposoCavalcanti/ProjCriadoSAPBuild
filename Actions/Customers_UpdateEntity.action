{
	"_Type": "Action.Type.ODataService.UpdateEntity",
	"ActionResult": {
		"_Name": "Customers_UpdateEntity"
	},
	"OnFailure": "/Myapp1Adr/Actions/UpdateCustomerEntityFailureMessage.action",
	"OnSuccess": "/Myapp1Adr/Actions/CloseModalPage_Complete.action",
	"ValidationRule": "/Myapp1Adr/Rules/EmailValidation.js",
	"Target": {
		"Service": "/Myapp1Adr/Services/com_sap_edm_sampleservice_v4.service",
		"EntitySet": "Customers",
		"ReadLink": "{@odata.readLink}"
	},
	"Properties": {
		"EmailAddress": "#Control:FCEmail/#Value",
		"FirstName": "#Control:FCFirstName/#Value",
		"LastName": "#Control:FCLastName/#Value",
		"PhoneNumber": "#Control:FCPhone/#Value"
	}
}